   "source": [
    "import polars as pl\n",
    "\n",
    "date ={'date': ['31-MAY-2024']}\n",
    "df = pl.DataFrame(date)\n",
    "\n",
    "df = df.with_columns(pl.col('date').str.strptime(pl.Date, '%d-%b-%Y'))\n",
    "\n",
    "print(df)\n",
    "\n"
   ]
  }
